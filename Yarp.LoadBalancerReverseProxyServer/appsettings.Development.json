{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ReverseProxy": {
    "Routes": {
      "pastpaper-api": {
        "ClusterId": "pastpaper-cluster",
        "Match": {
          "Path": "{**catchall}"
        }
      }
    },
    "Clusters": {
      "pastpaper-cluster": {
        "LoadBalancingPolicy": "RoundRobin",
        "Destinations": {
          "destination1": {
            "Address": "https://localhost:44311/"
          }
        }
      }
    }
  }
}
